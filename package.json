{
  "name": "webvr-gearvr-test",
  "version": "1.0.0",
  "description": "a simple test of WebVR running natively in GearVR",
  "main": "index.js",
  "license": "MIT",
  "author": {
    "name": "Matt DesLauriers",
    "email": "dave.des@gmail.com",
